{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
     "typeRoots": ["./node_modules/@types", "./constants"],
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@env": ["constants/env.d.ts"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}